/* 导航 */
.g-nav {
  position: fixed;
  z-index: 999;
  width: 750rpx;
  height: 200rpx;
  overflow: auto;
  background-color: #fff;
  border-bottom: 20rpx solid #f0f6f6;
  border-radius: 32rpx 32rpx 0 0;
}
.g-nav-box {
  display: flex;
  justify-content: space-around;
  white-space: nowrap;
  width: auto;
}
.g-nav-box ._span {
  /* width:140rpx; */
  padding: 0 35rpx;
  height: 88rpx;
  text-align: center;
  display: inline-block;
  font-size: 28rpx;
  color: #9a9a9a;
  height: 50rpx;
  line-height: 50rpx;
}
.nav .cu-item {
  height: 60rpx;
